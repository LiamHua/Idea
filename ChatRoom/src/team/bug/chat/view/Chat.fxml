<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="541.0" prefWidth="845.0" xmlns="http://javafx.com/javafx/8.0.231" xmlns:fx="http://javafx.com/fxml/1" fx:controller="team.bug.chat.controller.Chat">
   <children>
      <Pane prefHeight="541.0" prefWidth="846.0">
         <children>
            <Pane fx:id="chatPane" layoutX="207.0" layoutY="90.0" prefHeight="336.0" prefWidth="535.0">
               <children>
                  <Button id="bt_sent" fx:id="bt_sent" layoutY="295.0" mnemonicParsing="false" onAction="#btsent_onclick" text="发送" />
                  <TextField fx:id="Text_sent" layoutX="60.0" layoutY="295.0" onKeyPressed="#sent_ENTER" prefHeight="28.0" prefWidth="339.0" />
                  <Button fx:id="bt_sentFile" layoutX="415.0" layoutY="295.0" mnemonicParsing="false" onAction="#onClick_sentFile" prefHeight="28.0" prefWidth="44.0" text="文件" />
                  <ListView fx:id="msgList" maxHeight="283.0" prefHeight="283.0" prefWidth="510.0">
                     <effect>
                        <Blend />
                     </effect>
                  </ListView>
               </children>
            </Pane>
            <ListView fx:id="friendList" layoutX="14.0" layoutY="90.0" onMouseClicked="#onClick_fList" prefHeight="322.0" prefWidth="171.0" />
            <Label fx:id="welcome" layoutX="14.0" layoutY="14.0" prefHeight="37.0" prefWidth="143.0" text="welcome">
               <font>
                  <Font name="Arial Italic" size="18.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </children>
</AnchorPane>
