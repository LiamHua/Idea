<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="596.0" prefWidth="1032.0" xmlns="http://javafx.com/javafx/8.0.231"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="per.liam.stumanager.controller.InsertStuC">
    <TableView fx:id="insertTable" editable="true" layoutX="66.0" layoutY="67.0" prefHeight="301.0" prefWidth="900.0"
               tableMenuButtonVisible="true">
        <columns>
            <TableColumn fx:id="snoCol" prefWidth="99.0" text="学号"/>
            <TableColumn fx:id="nameCol" prefWidth="83.0" text="姓名"/>
            <TableColumn fx:id="sexCol" prefWidth="43.0" text="性别"/>
            <TableColumn fx:id="telCol" prefWidth="101.0" text="电话"/>
            <TableColumn fx:id="birthdayCol" prefWidth="126.0" text=" 出生年份"/>
            <TableColumn fx:id="addressCol" prefWidth="130.0" text="家庭住址"/>
            <TableColumn fx:id="instituteCol" minWidth="0.0" prefWidth="106.0" text="学院"/>
            <TableColumn fx:id="majorCol" prefWidth="123.0" text="专业"/>
            <TableColumn fx:id="startYearCol" prefWidth="59.0" text="入学年份"/>
        </columns>
    </TableView>
    <TextField fx:id="sno" layoutX="66.0" layoutY="376.0" prefHeight="28.0" prefWidth="86.0"/>
    <TextField fx:id="name" layoutX="152.0" layoutY="376.0" prefHeight="28.0" prefWidth="80.0"/>
    <TextField fx:id="tel" layoutX="292.0" layoutY="376.0" prefHeight="28.0" prefWidth="109.0"/>
    <TextField fx:id="birthday" layoutX="401.0" layoutY="376.0" prefHeight="28.0" prefWidth="109.0"/>
    <TextField fx:id="address" layoutX="508.0" layoutY="376.0" prefHeight="28.0" prefWidth="133.0"/>
    <Button layoutX="493.0" layoutY="422.0" mnemonicParsing="false" onAction="#onClickAdd" text="添加"/>
    <Button layoutX="447.0" layoutY="481.0" mnemonicParsing="false" onAction="#onClickUpdate" prefHeight="52.0"
            prefWidth="154.0" text="确认提交 ">
        <font>
            <Font size="20.0"/>
        </font>
    </Button>
    <ComboBox fx:id="sexChoice" layoutX="232.0" layoutY="375.0" prefHeight="30.0" prefWidth="62.0"
              style="-fx-background-color: WHITE; -fx-border-width: 1; -fx-border-color: GREY; -fx-border-radius: 3;"/>
    <ComboBox fx:id="instituteChoice" layoutX="638.0" layoutY="376.0" prefHeight="28.0" prefWidth="118.0"
              style="-fx-background-color: WHITE; -fx-border-width: 1; -fx-border-color: GREY; -fx-border-radius: 3;"/>
    <ComboBox fx:id="majorChoice" layoutX="756.0" layoutY="376.0" prefHeight="28.0" prefWidth="109.0"
              style="-fx-background-color: WHITE; -fx-border-width: 1; -fx-border-color: GREY; -fx-border-radius: 3;"/>
    <ComboBox fx:id="startYearChoice" layoutX="865.0" layoutY="376.0" prefHeight="28.0" prefWidth="98.0"
              style="-fx-background-color: WHITE; -fx-border-width: 1; -fx-border-color: GREY; -fx-border-radius: 3;"/>
</AnchorPane>
